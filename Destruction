local function AFPUR_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local Button = script.Parent
	
	Button.MouseButton1Click:Connect(function()
		setclipboard("https://scriptblox.com/u/PabloScripter")
		
	end)
end
coroutine.wrap(AFPUR_fake_script)()
