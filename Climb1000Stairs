local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local stairsFolder = workspace:FindFirstChild("Stairs")
local badgesFolder = workspace:WaitForChild("Badges")


local tweenService = game:GetService("TweenService")
local tweensInProgress = {}


local function tweenToPlayer()

    statusLabel.Text = "Status: Moving..."
    statusLabel.TextColor3 = Color3.fromRGB(0, 255, 0)

    local batchSize = tonumber(batchSlider.Text) or 10
    local speed = tonumber(speedSlider.Text) or 1
    local stairs = {}
    local badges = {}


    if stairsFolder then
        for _, stairPart in ipairs(stairsFolder:GetChildren()) do
            if stairPart:IsA("Part") then
                table.insert(stairs, stairPart)
            end
        end
    end

    if badgesFolder then
        for _, badgePart in ipairs(badgesFolder:GetChildren()) do
            if badgePart:IsA("Part") then
                table.insert(badges, badgePart)
            end
        end
    end

    local function tweenBatch(batch)
        for _, part in ipairs(batch) do
            part.CanCollide = false  -- Disable collision
            local playerPosition = humanoidRootPart.Position
            local tweenInfo = TweenInfo.new(
                speed, 
                Enum.EasingStyle.Linear,
                Enum.EasingDirection.InOut,
                -1,  
                false,  
                0  
            )
            local goal = {Position = playerPosition}
            local tween = tweenService:Create(part, tweenInfo, goal)
            tween:Play()

            
            table.insert(tweensInProgress, tween)
        end
    end

    
    local function startTweening()
        for i = 1, #stairs, batchSize do
            local stairBatch = {unpack(stairs, i, math.min(i + batchSize - 1, #stairs))}
            tweenBatch(stairBatch)
            wait(0.1) 
        end

        for i = 1, #badges, batchSize do
            local badgeBatch = {unpack(badges, i, math.min(i + batchSize - 1, #badges))}
            tweenBatch(badgeBatch)
            wait(0.1)
        end
    end

    startTweening()
end


startButton.MouseButton1Click:Connect(function()
    tweenToPlayer()
end)


stopButton.MouseButton1Click:Connect(function()
    for _, tween in ipairs(tweensInProgress) do
        tween:Pause()  -- Stop tweening
    end
    statusLabel.Text = "Status: Stopped"
    statusLabel.TextColor3 = Color3.fromRGB(255, 0, 0)
end)

-- Close Button functionality
closeButton.MouseButton1Click:Connect(function()
    screenGui:Destroy()
end)


local dragging = false
local startPos, startMousePos

uiFrame.InputBegan:Connect(function(input, gameProcessed)
    if gameProcessed then return end
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        startPos = uiFrame.Position
        startMousePos = input.Position
    end
end)

uiFrame.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local delta = input.Position - startMousePos
        uiFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

uiFrame.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)


end


local function checkGame()
    local currentGameId = game.PlaceId

    if currentGameId == correctGameId then
        rightgame()
    else
        game.Players.LocalPlayer:Kick("You're not in the game Climb 1000 Stairs Bc Pablo Is Proüòé ")
    end
end

checkGame()
